    "fontisto": "^3.0.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "sass": "^1.38.1",
    "sass-loader": "^10.1.1",
    "tippy.js": "^6.3.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
