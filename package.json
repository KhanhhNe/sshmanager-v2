{
  "name": "sshmanager",
  "description": "Quản lý SSH chuyên nghiệp và nhanh chóng",
  "version": "2.1.8",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest=build/web_dist",
    "build-watch": "vue-cli-service build --dest=build/web_dist --mode=development --watch --no-clean",
    "lint": "vue-cli-service lint"
